  "dependencies": {
    "JSONStream": "^1.3.5",
    "csv-streamify": "^4.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.19.1",
    "eslint-config-netflix": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^26.6.3",
    "typescript": "^4.2.2"
  }
