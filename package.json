  "dependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "JSONStream": "^1.3.5",
    "camelcase-keys-recursive": "^1.0.0",
