{
  "name": "@carloscasalar/eddb-node-adapter",
  "version": "0.5.0",
  "description": "Node adapter for EDDB API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint index.js **/*.js",
    "lint:fix": "eslint index.js **/*.js --fix",
    "example:commodities": "node ./examples/commodities",
    "example:populatedSystems": "node ./examples/populatedSystems",
    "example:prices": "node ./examples/prices",
    "example:stations": "node ./examples/stations"
