{
  "name": "eddb-node-adapter",
  "version": "0.4.0-snapshot",
  "description": "Node adapter for EDDB API",
  "main": "index.js",
  "scripts": {
    "test": "jest --verbose",
    "lint": "eslint index.js commodities/**",
    "example::commodities": "node ./examples/commodities",
    "example::populatedSystems": "node ./examples/populatedSystems",
    "example::prices": "node ./examples/prices"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carloscasalar/ed-node-adapter.git"
  },
  "keywords": [
    "eddb"
  ],
  "author": "Carlos Castillo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carloscasalar/ed-node-adapter/issues"
  },
  "homepage": "https://github.com/carloscasalar/ed-node-adapter#readme",
  "dependencies": {
    "JSONStream": "^1.3.5",
    "csv-streamify": "^4.0.0",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-netflix": "^2.0.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0"
  },
  "eslintConfig": {
    "extends": "netflix",
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "semi": [
        "off"
      ],
      "no-extra-semi": [
        "error"
      ],
      "class-methods-use-this": [
        "off"
      ]
    }
  }
}
